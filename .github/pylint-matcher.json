{
	"problemMatcher": [
		{
			"owner": "pylint-warning",
			"severity": "warning",
			"pattern": [
				{
					"regexp": "^(.+):(\\d+):(\\d+): ([CRW]\\d{4}): (.+) \\(\\S+\\)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"code": 4,
					"message": 5
				}
			]
		},
		{
			"owner": "pylint-error",
			"severity": "error",
			"pattern": [
				{
					"regexp": "^(.+):(\\d+):(\\d+): ([EF]\\d{4}): (.+) \\(\\S+\\)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"code": 4,
					"message": 5
				}
			]
		}
	]
}
